<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--主页面底部栏按钮自定义属性-->
    <declare-styleable name="BottomNavigationTab">
        <attr name="iconSize" format="dimension" />
        <attr name="textSize" format="dimension" />
        <attr name="tabText" format="string" />
        <attr name="iconAndTextPadding" format="dimension" />
        <attr name="defaultColor" format="color" />
        <attr name="pressedColor" format="color" />
        <attr name="defaultIcon" format="reference" />
        <attr name="pressedIcon" format="reference" />
    </declare-styleable>
    <declare-styleable name="ComplexView">
        <attr name="cvGravity" format="enum">
            <enum name="center" value="0" />
            <enum name="left" value="1" />
            <enum name="right" value="2" />
            <enum name="top" value="3" />
            <enum name="bottom" value="4" />
        </attr>

        <attr name="cvSelector" format="enum">
            <enum name="none" value="0" />
            <enum name="standard" value="1" />
            <enum name="ripple" value="2" />
        </attr>

        <attr name="cvNormalBgColor" format="color" />
        <attr name="cvPressedBgColor" format="color" />
        <attr name="cvDisableBgColor" format="color" />
        <attr name="cvNormalTextColor" format="color" />
        <attr name="cvPressedTextColor" format="color" />
        <attr name="cvDisableTextColor" format="color" />

        <attr name="cvCornersRadius" format="dimension" />
        <attr name="cvCornersTopLeftRadius" format="dimension" />
        <attr name="cvCornersTopRightRadius" format="dimension" />
        <attr name="cvCornersBottomLeftRadius" format="dimension" />
        <attr name="cvCornersBottomRightRadius" format="dimension" />

        <attr name="cvStrokeWidth" format="dimension" />
        <attr name="cvStrokeColor" format="color" />

        <attr name="cvElevation" format="dimension" />
        <attr name="cvClickable" format="boolean" />

        <attr name="cvIconStart" format="reference" />
        <attr name="cvIconTop" format="reference" />
        <attr name="cvIconEnd" format="reference" />
        <attr name="cvIconBottom" format="reference" />
        <attr name="cvIconSizeStart" format="dimension" />
        <attr name="cvIconSizeTop" format="dimension" />
        <attr name="cvIconSizeEnd" format="dimension" />
        <attr name="cvIconSizeBottom" format="dimension" />
        <attr name="cvTintIconStart" format="color" />
        <attr name="cvTintIconTop" format="color" />
        <attr name="cvTintIconEnd" format="color" />
        <attr name="cvTintIconBottom" format="color" />

        <attr name="cvIconPadding" format="dimension" />
    </declare-styleable>
    <declare-styleable name="ToggleButton">
        <attr name="borderWidth" format="dimension" />
        <attr name="offBorderColor" format="reference|color" />
        <attr name="offColor" format="reference|color" />
        <attr name="onColor" format="reference|color" />
        <attr name="spotColor" format="reference|color" />
        <attr name="animate" format="reference|boolean" />
        <attr name="isDefaultOn" format="reference|boolean" />
    </declare-styleable>
    <declare-styleable name="AvatarImageView">
